{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "scrollbar-width",
          "scrollbar-width: none"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off"
  }
}